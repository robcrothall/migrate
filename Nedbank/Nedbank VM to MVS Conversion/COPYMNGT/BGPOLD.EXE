'FILEDEF ICBG05 DISK ICBG05 ASFILE B (RECFM FB LRECL 60 BLOCK 9000'
'FILEDEF ICBG7A DISK ICBG7A ASFILE B (RECFM FB LRECL 100 BLOCK 18000'
'FILEDEF ICBG08 DISK ICBG08 ASFILE B (RECFM FB LRECL 40 BLOCK 7200'
'FILEDEF ICBG09 DISK ICBG09 ASFILE B (RECFM FB LRECL 40 BLOCK 7200'
'FILEDEF ICBG10 DISK ICBG10 ASFILE B (RECFM FB LRECL 40 BLOCK 7200'
'FILEDEF ICBG11 DISK ICBG11 ASFILE B (RECFM FB LRECL 110 BLOCK 27940'

'EXEC SQLDBSU IN(BGP ACQUIRE) PR(BGP PRACQ)'
'EXEC SQLDBSU IN(BGP CREATE)  PR(BGP PRCREATE)'
'EXEC SQLDBSU IN(BGP LOAD)    PR(BGP PRLOAD)'
'EXEC SQLDBSU IN(BGP INDEX)   PR(BGP PRINDEX)'
'EXEC SQLDBSU IN(BGP VIEW)    PR(BGP PRVIEW)'
